<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myhealth"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myhealth.MainActivity"
            android:label="@string/app_name" >
        </activity>
     
        <activity
            android:name="com.example.myhealth.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        
         </activity> 		  <activity android:name="com.example.myhealth.Menu_Activity"></activity>
        <activity android:name="com.example.myhealth.Measurements"></activity>
        <activity android:name="com.example.myhealth.UrineTest"></activity>
        <activity android:name="com.example.myhealth.Device"></activity>
       
    </application>

</manifest>